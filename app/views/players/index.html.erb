<h1>Listing players</h1>

<table>
  <tr>
  </tr>

<% @players.each do |player| %>
  <tr>
		<td><%= player.number %></td>
		<td><%= player.name %></td>
		<td><%= player.position %></td>
		<td><%= player.height %></td>
		<td><%= player.weight %></td>
		<td><%= player.year %></td>
		<td><%= player.hometown %></td>
		<td><%= player.previous_school %></td>
    <td><%= link_to 'Edit', edit_player_path(player) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New player', new_player_path %>